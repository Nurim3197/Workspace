ALTER SESSION SET "_ORACLE_SCRIPT" = TRUE;
CREATE USER MATE IDENTIFIED BY 1234;
ALTER USER MATE DEFAULT TABLESPACE users QUOTA UNLIMITED ON users;
ALTER USER MATE TEMPORARY TABLESPACE temp;
GRANT CREATE SESSION, CREATE VIEW, ALTER SESSION, CREATE SEQUENCE TO MATE;
GRANT CREATE SYNONYM, CREATE DATABASE LINK, RESOURCE, UNLIMITED TABLESPACE TO MATE;

COMMIT;
