CREATE TABLE "USR_INF" (
	"USR_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"USR_LGN_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"USR_PWD"	VARCHAR2 (20 CHAR)		NOT NULL,
	"USR_LNM"	VARCHAR2 (20 CHAR)		NOT NULL,
	"USR_FNM"	VARCHAR2 (20 CHAR)		NOT NULL,
	"USR_GNDR"	VARCHAR2 (6 CHAR)		NOT NULL,
	"USR_EML"	VARCHAR2 (100 CHAR)		NOT NULL,
	"USR_PHN"	VARCHAR2 (20 CHAR)		NOT NULL,
	"USR_BD"	DATE		NOT NULL,
	"USR_ADDRS"	VARCHAR2 (200 CHAR)		NOT NULL,
	"USR_IS_GD"	CHAR(1)	DEFAULT 'N'	NOT NULL,
	"USR_SLF_INTDCTN"	CLOB		NULL,
	"USR_PYP_EML"	VARCHAR2 (100 CHAR)		NULL,
	"USR_AVG_RTNG"	NUMBER (2, 1)		NULL,
	"USR_JN_DT"	DATE		NOT NULL,
	"USR_CL_DT"	DATE		NULL,
	"USR_IS_CL"	CHAR (1)	DEFAULT 'N'	NOT NULL,
	"USR_MDFY_DT"	DATE		NULL,
	"GD_PRFL_IMG"	VARCHAR2 (1000 CHAR)		NULL,
	"GD_ID_IMG"	VARCHAR2 (1000 CHAR)		NULL,
	"GD_CBC_IMG"	VARCHAR2 (1000 CHAR)		NULL,
	"GD_RP_CNT_ID"	VARCHAR2 (20 CHAR)		NULL
);

COMMENT ON COLUMN "USR_INF"."USR_ID" IS '회원의 PK';

COMMENT ON COLUMN "USR_INF"."USR_LGN_ID" IS '회원의 로그인 아이디';

COMMENT ON COLUMN "USR_INF"."USR_PWD" IS '회원의 로그인 비밀번호';

COMMENT ON COLUMN "USR_INF"."USR_LNM" IS '회원 이름의 성';

COMMENT ON COLUMN "USR_INF"."USR_FNM" IS '유저의 이름';

COMMENT ON COLUMN "USR_INF"."USR_GNDR" IS '회원의 성별(MALE, FEMALE)';

COMMENT ON COLUMN "USR_INF"."USR_EML" IS '회원의 이메일';

COMMENT ON COLUMN "USR_INF"."USR_PHN" IS '회원의 전화번호';

COMMENT ON COLUMN "USR_INF"."USR_BD" IS '회원의 생년월일';

COMMENT ON COLUMN "USR_INF"."USR_ADDRS" IS '회원의 주소';

COMMENT ON COLUMN "USR_INF"."USR_IS_GD" IS '여행자 / 가이드 구분
가이드면 TRUE';

COMMENT ON COLUMN "USR_INF"."USR_SLF_INTDCTN" IS '가이드의 자기소개';

COMMENT ON COLUMN "USR_INF"."USR_PYP_EML" IS '가이드의 정산 계좌 메일';

COMMENT ON COLUMN "USR_INF"."USR_AVG_RTNG" IS '가이드의 평균 별점';

COMMENT ON COLUMN "USR_INF"."USR_JN_DT" IS '회원의 가입일자';

COMMENT ON COLUMN "USR_INF"."USR_CL_DT" IS '회원의 탈퇴일자';

COMMENT ON COLUMN "USR_INF"."USR_IS_CL" IS '회원의 탈퇴 여부';

COMMENT ON COLUMN "USR_INF"."USR_MDFY_DT" IS '회원 정보 수정 일자';

COMMENT ON COLUMN "USR_INF"."GD_PRFL_IMG" IS '가이드의 프로필 사진 URL';

COMMENT ON COLUMN "USR_INF"."GD_ID_IMG" IS '가이드의 신분증 사진 URL';

COMMENT ON COLUMN "USR_INF"."GD_CBC_IMG" IS '가이드의 범죄경력조회서 사진 URL';

COMMENT ON COLUMN "USR_INF"."GD_RP_CNT_ID" IS 'FK. 회원이 출생한 국가 아이디';

CREATE TABLE "MNGR" (
	"MAN_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"MAN_LNM"	VARCHAR2 (20 CHAR)		NOT NULL,
	"MAN_FNM"	VARCHAR2 (50 CHAR)		NOT NULL,
	"MAN_GRD"	VARCHAR2 (50 CHAR)		NOT NULL,
	"MAN_PHN"	VARCHAR2 (20 CHAR)		NOT NULL,
	"MAN_LGN_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"MAN_PWD"	VARCHAR2 (20 CHAR)		NOT NULL,
	"MAN_EML"	VARCHAR2 (100 CHAR)		NOT NULL,
	"MAN_IS_CHF"	CHAR (1)	DEFAULT 'N'	NOT NULL,
	"MAN_IS_DLT"	CHAR (1)	DEFAULT 'N'	NOT NULL,
	"MAN_RGSTR_DT"	DATE		NOT NULL,
	"MAN_DLT_DT"	DATE		NULL,
	"MAN_MDFY_DT"	DATE		NULL
);

COMMENT ON COLUMN "MNGR"."MAN_ID" IS '관리자 정보의 PK';

COMMENT ON COLUMN "MNGR"."MAN_LNM" IS '관리자 이름의 성';

COMMENT ON COLUMN "MNGR"."MAN_FNM" IS '관리자의 이름';

COMMENT ON COLUMN "MNGR"."MAN_GRD" IS '관리자의 직급';

COMMENT ON COLUMN "MNGR"."MAN_PHN" IS '관리자의 전화번호';

COMMENT ON COLUMN "MNGR"."MAN_LGN_ID" IS '관리자의 로그인 아이디';

COMMENT ON COLUMN "MNGR"."MAN_PWD" IS '관리자의 로그인 패스워드';

COMMENT ON COLUMN "MNGR"."MAN_EML" IS '관리자의 이메일';

COMMENT ON COLUMN "MNGR"."MAN_IS_CHF" IS '관리자의 총관리자 여부(TRUE면 총관리자)';

COMMENT ON COLUMN "MNGR"."MAN_IS_DLT" IS '관리자 정보의 삭제 여부';

COMMENT ON COLUMN "MNGR"."MAN_RGSTR_DT" IS '관리자가 등록된 일시';

COMMENT ON COLUMN "MNGR"."MAN_DLT_DT" IS '관리자 정보가 삭제된 일시';

COMMENT ON COLUMN "MNGR"."MAN_MDFY_DT" IS '관리자 정보가 수정된 일시';

CREATE TABLE "GD_DCMNT" (
	"GD_LCN_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"GD_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"GD_LCN_NM"	VARCHAR2 (100 CHAR)		NOT NULL,
	"GD_LCN_IMG"	VARCHAR2 (1000 CHAR)		NOT NULL
);

COMMENT ON COLUMN "GD_DCMNT"."GD_LCN_ID" IS '가이드의 라이센스 아이디';

COMMENT ON COLUMN "GD_DCMNT"."GD_ID" IS '가이드의 PK';

COMMENT ON COLUMN "GD_DCMNT"."GD_LCN_NM" IS '가이드 라이센스 이름';

COMMENT ON COLUMN "GD_DCMNT"."GD_LCN_IMG" IS '가이드 라이센스 이미지 URL';

CREATE TABLE "CNT" (
	"CNT_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"CNT_NM"	VARCHAR2 (50 CHAR)		NOT NULL
);

COMMENT ON COLUMN "CNT"."CNT_ID" IS '국가 테이블의 PK';

COMMENT ON COLUMN "CNT"."CNT_NM" IS '국가명';

CREATE TABLE "CT" (
	"CT_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"CNT_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"CT_NM"	VARCHAR2 (20 CHAR)		NOT NULL
);

COMMENT ON COLUMN "CT"."CT_ID" IS '도시 테이블의 PK';

COMMENT ON COLUMN "CT"."CNT_ID" IS '국가 테이블의 PK';

COMMENT ON COLUMN "CT"."CT_NM" IS '도시명';

CREATE TABLE "GD_TR_RCRT" (
	"GD_TR_PST_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"ATHR_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"USR_ID"	VARCHAR2 (20 CHAR)		NULL,
	"GD_TR_TTL"	VARCHAR2 (500 CHAR)		NOT NULL,
	"GD_TR_DT"	DATE		NOT NULL,
	"GD_TR_PRPS"	VARCHAR2 (500 CHAR)		NOT NULL,
	"GD_TR_MP"	VARCHAR2 (1000 CHAR)		NOT NULL,
	"GD_TR_PRC"	NUMBER (4, 2)		NOT NULL,
	"GD_TR_SMRY"	CLOB		NULL,
	"GD_TR_TM"	NUMBER (2, 1)		NOT NULL,
	"GD_TR_RSTR_DT"	DATE		NOT NULL,
	"GD_TR_MDFY_DT"	DATE		NULL,
	"GD_TR_DLT_DT"	DATE		NULL,
	"GD_TR_IS_DLT"	CHAR (1)	DEFAULT 'N'	NOT NULL,
	"TR_CNT_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"TR_CT_ID"	VARCHAR2 (20 CHAR)		NOT NULL
);

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_PST_ID" IS '가이드가 등록한 투어 게시글 아이디';

COMMENT ON COLUMN "GD_TR_RCRT"."ATHR_ID" IS '작성자(가이드)의 PK';

COMMENT ON COLUMN "GD_TR_RCRT"."USR_ID" IS '예약한 회원의 PK';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_TTL" IS '가이드가 등록한 투어 제목';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_DT" IS '투어 시작 일자';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_PRPS" IS '투어 목적';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_MP" IS '여행자와 만날 장소';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_PRC" IS '투어 가격';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_SMRY" IS '투어 정보 요약';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_TM" IS '투어 소요 시간(시간단위)';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_RSTR_DT" IS '투어 게시글 등록 날자';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_MDFY_DT" IS '투어 게시글 수정일';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_DLT_DT" IS '투어 게시글 삭제일';

COMMENT ON COLUMN "GD_TR_RCRT"."GD_TR_IS_DLT" IS '투어 게시글 삭제여부';

COMMENT ON COLUMN "GD_TR_RCRT"."TR_CNT_ID" IS '투어를 진행할 국가 아이디 fk';

COMMENT ON COLUMN "GD_TR_RCRT"."TR_CT_ID" IS '투어를 진행할 도시 아이디 fk';

CREATE TABLE "USR_TR_RQST" (
	"USR_TR_PST_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"ATHR_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"GD_ID"	VARCHAR2 (20 CHAR)		NULL,
	"USR_TR_TTL"	VARCHAR2 (500 CHAR)		NOT NULL,
	"USR_TR_DT"	DATE		NOT NULL,
	"USR_TR_PRPS"	VARCHAR2 (1000 CHAR)		NOT NULL,
	"USR_TR_MP"	VARCHAR2 (1000 CHAR)		NOT NULL,
	"USR_TR_GD_RQ"	VARCHAR2 (1000 CHAR)		NULL,
	"USR_TR_GD_HR_PRC"	NUMBER (4, 2)		NOT NULL,
	"USR_TR_NP"	NUMBER (2,0)		NOT NULL,
	"USR_TR_IS_DLT"	CHAR (1)	DEFAULT 'N'	NOT NULL,
	"USR_TR_TM"	NUMBER (2,1)		NOT NULL,
	"USR_TR_RSTR_DT"	DATE		NOT NULL,
	"USR_TR_MDFY_DT"	DATE		NULL,
	"USR_TR_DLT_DT"	DATE		NULL,
	"TR_CT_ID"	VARCHAR2 (20 CHAR)		NOT NULL
);

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_PST_ID" IS '게시글의 아이디. Pk';

COMMENT ON COLUMN "USR_TR_RQST"."ATHR_ID" IS '작성자(여행자)의 PK';

COMMENT ON COLUMN "USR_TR_RQST"."GD_ID" IS '가이드의 PK';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_TTL" IS '여행자가 등록한 투어 게시글의 제목';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_DT" IS '여행자가 입력한 투어 일시';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_PRPS" IS '여행자가 입력한 투어의 목적';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_MP" IS '여행자가 등록한 가이드와 만날 지점에 대한 정보';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_GD_RQ" IS '가이드에게 간단하게 요청할 사안';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_GD_HR_PRC" IS '여행자가 가이드에게 제시하는 고용 금액';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_NP" IS '여행자의 총 투어 인원';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_IS_DLT" IS '논리적 삭제를 위한 게시글 삭제 여부';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_TM" IS '여행자의 투어 소요 시간(시간 단위)';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_RSTR_DT" IS '여행자가 투어 요청 게시글을 등록한 일시';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_MDFY_DT" IS '여행자가 투어 요청 게시글을 수정한 일시';

COMMENT ON COLUMN "USR_TR_RQST"."USR_TR_DLT_DT" IS '여행자가 투어 요청 게시글을 삭제한 일시';

COMMENT ON COLUMN "USR_TR_RQST"."TR_CT_ID" IS '여행자가 등록한 투어 지역의 도시명. FK';

CREATE TABLE "GD_RVW" (
	"GD_RVW_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"ATHR_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"GD_ID"	VARCHAR2 (20 CHAR)		NOT NULL,
	"GD_RVW_TTL"	VARCHAR2 (100 CHAR)		NOT NULL,
	"GD_RVW_CNTNT"	VARCHAR2 (1000 CHAR)		NOT NULL,
	"GD_RVW_CRTDAT"	DATE		NOT NULL,
	"GD_RVW_DLT_AT"	DATE		NULL,
	"GD_RVW_IS_DLT"	CHAR (1)	DEFAULT 'N'	NOT NULL,
	"GD_RVW_RTNG"	NUMBER (2, 1)		NOT NULL
);

COMMENT ON COLUMN "GD_RVW"."GD_RVW_ID" IS '가이드 리뷰 아이디';

COMMENT ON COLUMN "GD_RVW"."ATHR_ID" IS '작성자의 PK';

COMMENT ON COLUMN "GD_RVW"."GD_ID" IS '가이드의 PK';

COMMENT ON COLUMN "GD_RVW"."GD_RVW_TTL" IS '리뷰의 제목';

COMMENT ON COLUMN "GD_RVW"."GD_RVW_CNTNT" IS '리뷰의 내용';

COMMENT ON COLUMN "GD_RVW"."GD_RVW_CRTDAT" IS '리뷰의 작성일자';

COMMENT ON COLUMN "GD_RVW"."GD_RVW_DLT_AT" IS '리뷰의 삭제일자';

COMMENT ON COLUMN "GD_RVW"."GD_RVW_IS_DLT" IS '리뷰의 삭제 여부';

COMMENT ON COLUMN "GD_RVW"."GD_RVW_RTNG" IS '가이드에 대한 평점';

