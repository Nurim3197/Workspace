package com.mate.message.vo;

public class MessageVO {
    private String msgId;
    private String msgInfo;
    private String msgSendTm;
    private boolean isChecked;
    private boolean isDeleted;
    private String deletedTm;
    private String msgSenderId;
    private String msgReciverId;

    public String getMsgId() {
        return msgId;
    }

    public void setMsgId(String msgId) {
        this.msgId = msgId;
    }

    public String getMsgInfo() {
        return msgInfo;
    }

    public void setMsgInfo(String msgInfo) {
        this.msgInfo = msgInfo;
    }

    public String getMsgSendTm() {
        return msgSendTm;
    }

    public void setMsgSendTm(String msgSendTm) {
        this.msgSendTm = msgSendTm;
    }

    public boolean isChecked() {
        return isChecked;
    }

    public void setChecked(boolean checked) {
        isChecked = checked;
    }

    public boolean isDeleted() {
        return isDeleted;
    }

    public void setDeleted(boolean deleted) {
        isDeleted = deleted;
    }

    public String getDeletedTm() {
        return deletedTm;
    }

    public void setDeletedTm(String deletedTm) {
        this.deletedTm = deletedTm;
    }

    public String getMsgSenderId() {
        return msgSenderId;
    }

    public void setMsgSenderId(String msgSenderId) {
        this.msgSenderId = msgSenderId;
    }

    public String getMsgReciverId() {
        return msgReciverId;
    }

    public void setMsgReciverId(String msgReciverId) {
        this.msgReciverId = msgReciverId;
    }
}
